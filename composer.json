{
	"name": "rackbeat/php",
	"description": "PHP Client Library for our API",
	"keywords": [],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
