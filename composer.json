	"require": {
		"php": ">=7.1",
		"guzzlehttp/guzzle": "~6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Rackbeat\\": "src/"
		}
